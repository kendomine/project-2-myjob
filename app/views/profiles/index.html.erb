<div class="container">
  <div class="col-xs-12">

    
    <div class="col-xs-12" id= "name"> 
     <%=@profile.full_name%>
    </div>
      <%= link_to "EDIT", profile_path(@profile.id)%>
    <div class="row ">
      <div class="col-xs-1">

      </div>
      <div class="col-xs-5" id ="avatar">
       2 of 4
      </div>
      <div class="col-xs-5" id ="linkedin">
        3 of 4
      </div>
      <div class="col-xs-1">

       </div>

    </div>



    <div class="col-xs-4" id ="number">
      <a id="square" class="fa phone-square fa-phone-square fa-2x" aria-label="linkedin"> </a> :
      <%=@profile.phone_number%>
    </div>
    <div class="col-xs-4" id ="email">
      <a id="envelope" class="fa fa-envelope fa-2x" aria-label="linkedin"> </a> :
      <%=@profile.email%>
    </div>
    <div class="col-xs-4" id ="address">
      <a id="card" class="fa fa-address-card fa-2x" aria-label="linkedin"> </a> :
      <%=@profile.address%>
    </div>



  </div>
</div>



